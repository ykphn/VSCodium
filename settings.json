{
    "editor.fontFamily": "JetBrains Mono",
    "editor.fontSize": 13,
    "files.autoSave": "afterDelay",
    "workbench.colorCustomizations": {
        "editorBracketMatch.border": "#ccc",
        "editorBracketHighlight.foreground1": "#ff0f",
        "editorBracketHighlight.foreground2": "#0fff",
        "editorBracketHighlight.foreground3": "#f0ff",
        "editorBracketHighlight.foreground4": "#ff0f",
        "editorBracketHighlight.foreground5": "#0fff",
        "editorBracketHighlight.foreground6": "#f0ff"
    },
    "window.restoreWindows": "none",
    "workbench.iconTheme": "material-icon-theme",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true
    },
    "git.autofetch": true,
    "workbench.colorTheme": "Dracula Theme Soft",
    "python.defaultInterpreterPath": "/bin/python",
    "window.customTitleBarVisibility": "auto",
    "window.titleBarStyle": "custom",
    "editor.minimap.enabled": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false
}
